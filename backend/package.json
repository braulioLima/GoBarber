{
  "name": "gobarber-api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only src/shared/infra/http/server.ts",
    "resetDB": "yarn typeorm schema:drop && yarn typeorm migration:run",
    "start": "ts-node src/shared/infra/http/server.ts",
    "docker-stop": "for db in gobarberDB mongodb redis; do docker stop $db; done",
    "docker-start": "for db in gobarberDB mongodb redis; do docker start $db; done",
    "docker-restart": "yarn docker-stop && yarn docker-start",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/handlebars": "^4.1.0",
    "@types/hapi__joi": "^17.1.0",
    "@types/ioredis": "^4.16.2",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongodb": "^3.5.18",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.11.1",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.4",
    "ts-jest": "^26.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/redis": "^2.8.21",
    "aws-sdk": "^2.683.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^12.1.1",
    "class-transformer": "^0.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.12.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.6",
    "ioredis": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mime": "^2.4.5",
    "mongodb": "^3.5.7",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.6",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "rate-limiter-flexible": "^2.1.6",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.2.0",
    "typeorm": "^0.2.24",
    "uuidv4": "^6.0.7"
  }
}
